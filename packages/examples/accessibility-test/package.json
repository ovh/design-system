{
  "name": "accessibility-test",
  "version": "19.5.0",
  "private": true,
  "description": "Running a11y checks on ODS components",
  "type": "module",
  "scripts": {
    "build": "tsc -b && vite build",
    "lighthouse": "lighthouse $URL --only-categories accessibility --output-path=./dist/lighthouse-results.html",
    "start": "vite",
    "start:prod": "npm run build && vite preview"
  },
  "dependencies": {
    "@ovhcloud/ods-react": "19.5.0",
    "@ovhcloud/ods-themes": "19.5.0",
    "classnames": "2.5.1",
    "react": "19.1.0",
    "react-dom": "19.1.0"
  },
  "devDependencies": {
    "@types/react": "19.1.0",
    "@types/react-dom": "19.1.1",
    "@vitejs/plugin-react": "4.3.4",
    "axe-core": "4.10.3",
    "lighthouse": "12.8.0",
    "typescript": "5.3.3",
    "vite": "6.2.0"
  }
}
