{
  "name": "@ovhcloud/ods-testing",
  "version": "13.0.0",
  "description": "ODS testing stuff",
  "author": "OVH SAS",
  "license": "Apache-2.0",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "scripts": {
    "ignore:rm": "git clean -Xdf",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:prod:esm": "tsc -p tsconfig.esm.prod.json",
    "build:prod:cjs": "tsc -p tsconfig.cjs.prod.json",
    "build": "npm run build:esm && npm run build:cjs",
    "build:prod": "npm run build:prod:esm && npm run build:prod:cjs",
    "watch": "tsc -w -p tsconfig.esm.json",
    "generate:licence": "npx generate-license-file --input package.json --output THIRD-PARTY-LICENCES --overwrite",
    "doc": "typedoc --plugin typedoc-plugin-markdown --hideBreadcrumbs true --hideInPageTOC true",
    "doc:api": "typedoc --plugin none src/*",
    "test:spec": "jest",
    "test:spec:ci": "jest --ci"
  },
  "peerDependencies": {
    "@ovhcloud/ods-core": ">=2",
    "@ovhcloud/ods-theming": ">=2",
    "@stencil/core": ">=2.6"
  },
  "devDependencies": {
    "@ovhcloud/ods-core": "^13.0.0",
    "@ovhcloud/ods-library-dev": "^13.0.0",
    "@ovhcloud/ods-theming": "^13.0.0",
    "jest-transform-stub": "^2.0.0"
  }
}
