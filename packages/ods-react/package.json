{
  "name": "@ovhcloud/ods-react",
  "version": "19.3.0",
  "description": "ODS React Components",
  "author": "OVH SAS",
  "license": "Apache-2.0",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "default": "./dist/ods-react.js"
    },
    "./normalize-css": {
      "default": "./src/style/normalize.css"
    },
    "./style": {
      "default": "./src/style/index.scss"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "sideEffects": [
    "*.css"
  ],
  "scripts": {
    "build:prod": "rimraf dist && tsc -b && vite build",
    "clean": "rimraf dist",
    "lint:scss": "stylelint 'src/style/*.scss'",
    "lint:ts": "eslint '{src,tests}/!(components)/**/*.{ts,tsx}'",
    "test:spec": "jest --passWithNoTests",
    "test:spec:ci": "npm run test:spec"
  },
  "dependencies": {
    "@ark-ui/react": "5.25.1",
    "classnames": "2.5.1",
    "libphonenumber-js": "1.12.8",
    "react-hot-toast": "2.6.0",
    "react-shiki": "0.9.0"
  },
  "peerDependencies": {
    "react": ">=18.2.0",
    "react-dom": ">=18.2.0"
  },
  "devDependencies": {
    "@storybook/react": "8.6.12",
    "@storybook/react-vite": "8.6.12",
    "@types/jest": "29.5.12",
    "@types/react": "18.2.56",
    "@types/react-dom": "18.2.25",
    "@vitejs/plugin-react": "4.3.4",
    "cssjson": "2.1.3",
    "glob": "11.0.1",
    "jest": "29.7.0",
    "jest-puppeteer": "10.0.1",
    "jest-transform-stub": "2.0.0",
    "puppeteer": "22.15.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "sass": "1.71.0",
    "start-server-and-test": "2.0.10",
    "storybook": "8.6.12",
    "ts-jest": "29.1.2",
    "ts-node": "10.9.2",
    "typedoc": "0.28.1",
    "typescript": "5.3.3",
    "vite": "6.2.0",
    "vite-plugin-dts": "4.5.0",
    "vite-plugin-lib-inject-css": "2.2.1"
  }
}
