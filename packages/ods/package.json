    "@types/vanillajs-datepicker": "1.3.5",
    "autoprefixer": "10.4.17",
    "concurrently": "8.2.2",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "jest-puppeteer": "10.0.1",
    "jest-transform-stub": "2.0.0",
    "node-fetch": "2.7.0",
    "postcss": "8.4.35",
    "puppeteer": "22.15.0",
    "replace-in-file": "7.1.0",
    "sass": "1.71.0",
    "ts-jest": "29.1.2",
    "ts-node": "10.9.2",
    "tsx": "4.17.0",
    "typescript": "5.3.3",
    "wait-on": "7.2.0"
  }
