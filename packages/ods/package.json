  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "es2015": "dist/esm/index.js",
  "es2017": "dist/esm/index.js",
  "jsnext:main": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "scripts": {
    "build:ci": "npm run build:prod",
    "build:prod": "node ./scripts/build-stencil",
    "build:react": "npm --prefix react run build",
    "build:stencil": "stencil build --prod --config stencil.config.ts",
    "build:style": "npm --prefix style run build",
    "build:vue": "npm --prefix vue run build",
    "clean": "rimraf dist",
    "lint:scss": "stylelint 'src/style/*.scss'",
    "lint:ts": "eslint '{src,tests}/!(components)/**/*.{ts,tsx}'",
    "test:spec": "jest --config jest.config.ts --runInBand --coverage",
    "test:spec:ci": "jest --config jest.config.ts --runInBand "
  },
  "dependencies": {
    "@floating-ui/dom": "1.6.11",
    "@stencil/core": "4.16.0",
    "google-libphonenumber": "3.2.35",
