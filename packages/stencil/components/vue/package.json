    "compile:esm": "tsc -p tsconfig.json",
    "compile:cjs": "tsc -p tsconfig.cjs.json"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist/"
  ],
  "dependencies": {
    "@ovhcloud/ods-stencil-components": "^12.1.0"
  },
  "peerDependencies": {
    "vue": ">=3"
  },
  "devDependencies": {
    "rimraf": "^3.0.2",
    "typescript": "4.7.4",
    "vue": "^3.2.41"
  }
}
