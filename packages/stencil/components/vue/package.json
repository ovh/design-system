{
  "name": "@ovhcloud/ods-stencil-components-vue",
  "version": "12.1.0",
  "private": true,
  "description": "Vue specific wrapper for ods",
  "keywords": [],
  "license": "Apache-2.0",
  "scripts": {
    "build": "npm run clean && npm run compile",
    "clean": "rimraf dist",
