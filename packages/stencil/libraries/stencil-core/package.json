{
  "name": "@ovhcloud/ods-stencil-core",
  "version": "12.0.5",
  "private": true,
  "description": "ODS core",
  "license": "Apache-2.0",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "scripts": {
    "ignore:rm": "git clean -Xdf",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build": "npm run build:esm && npm run build:cjs",
    "build:prod": "npm run build",
    "watch": "tsc -w -p tsconfig.esm.json",
    "generate:licence": "npx generate-license-file --input package.json --output THIRD-PARTY-LICENCES --overwrite",
    "doc:api": "typedoc --plugin none src/*"
  },
  "peerDependencies": {
    "@ovhcloud/ods-core": ">=2",
    "@stencil/core": "~2.18.1",
    "@stencil/postcss": "~2.1.0",
    "@stencil/sass": "~2.0.0",
    "autoprefixer": "~10.3.7"
  },
  "devDependencies": {
    "@ovhcloud/ods-core": "^12.0.5",
    "@stencil/core": "~2.18.1",
    "@stencil/postcss": "~2.1.0",
    "@stencil/react-output-target": "~0.2.1",
    "@stencil/sass": "~2.0.0",
    "@stencil/vue-output-target": "~0.6.2",
    "@types/autoprefixer": "~10.2.0",
    "autoprefixer": "~10.3.7",
    "postcss": "^8.3.9",
    "typescript": "4.7.4"
  },
  "browserslist": [
    "last 4 version",
    "maintained node versions"
  ]
}
