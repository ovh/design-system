{
  "name": "@ovhcloud/ods-stencil-testing",
  "version": "13.0.1",
  "private": true,
  "description": "ODS core",
  "license": "Apache-2.0",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "scripts": {
    "ignore:rm": "git clean -Xdf",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build": "npm run build:esm && npm run build:cjs",
    "build:prod": "npm run build",
    "watch": "tsc -w -p tsconfig.esm.json",
    "generate:licence": "npx generate-license-file --input package.json --output THIRD-PARTY-LICENCES --overwrite",
    "doc:api": "typedoc --plugin none src/*"
  },
  "peerDependencies": {
    "@ovhcloud/ods-core": ">=9.1",
    "@stencil/core": "~2.18.1",
    "puppeteer": "~10.0.0"
  },
  "devDependencies": {
    "@ovhcloud/ods-core": "^13.0.1",
    "@ovhcloud/ods-testing": "^13.0.1",
    "@stencil/core": "~2.18.1",
    "typescript": "4.7.4"
  }
}
