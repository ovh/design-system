{
  "name": "@ovhcloud/ods-storybook",
  "version": "17.0.3",
  "description": "ODS Storybook",
  "author": "OVH SAS",
  "license": "Apache-2.0",
  "main": "index.js",
  "scripts": {
    "build:storybook": "storybook build -o dist",
    "__list:releases": "node .storybook/utils/get-repo-releases.js",
    "__prepare": "npm run build:storybook",
    "__start": "yarn list:releases && storybook dev -p 6006",
    "start": "storybook dev -p 6006",
    "__start:nocache": "yarn list:releases && storybook dev -p 6006 --no-manager-cache",
    "__start:quiet": "yarn list:releases && storybook dev -p 6006 --quiet"
  },
  "dependencies": {
    "@ovhcloud/ods-components": "17.1.0",
    "react": "18.2.0"
  },
  "devDependencies": {
    "@storybook/addon-a11y": "8.0.4",
    "@storybook/addon-essentials": "8.0.4",
    "@storybook/addon-links": "8.0.4",
    "@storybook/blocks": "8.0.4",
    "@storybook/test": "8.0.4",
    "@storybook/web-components": "8.0.4",
    "@storybook/web-components-vite": "8.0.4",
    "lit": "3.1.2",
    "lit-html": "3.1.2",
    "prop-types": "15.8.1",
    "storybook": "8.0.4",
    "typescript": "5.3.3"
  }
}
