{
  "name": "@ovhcloud/ods-storybook",
  "version": "19.4.1",
  "description": "ODS Storybook",
  "author": "OVH SAS",
  "license": "Apache-2.0",
  "main": "index.js",
  "scripts": {
    "build:storybook": "yarn generate:versions && storybook build -o dist",
    "extract:llms": "storybook-llms-extractor --config llms.config.js && node scripts/postprocess-llms.js",
    "install:browsers": "playwright install --with-deps chromium",
    "generate:versions": "node scripts/generate-version-list.js 'assets/ods-versions.json'",
    "start": "yarn generate:versions && storybook dev -p 6006 --no-open"
  },
  "dependencies": {
    "@ant-design/colors": "7.1.0",
    "@ark-ui/react": "5.25.1",
    "@ovhcloud/ods-react": "19.4.1",
    "@ovhcloud/ods-recipes": "19.4.1",
    "@ovhcloud/ods-themes": "19.4.1",
    "classnames": "2.5.1",
    "lz-string": "1.5.0",
    "prettier": "3.6.2",
    "react": "18.2.0",
    "storybook-addon-code-editor": "4.1.2",
    "typedoc": "0.28.1"
  },
  "devDependencies": {
    "@fluentui/storybook-llms-extractor": "0.0.3",
    "@storybook/addon-essentials": "8.6.12",
    "@storybook/addon-links": "8.6.12",
    "@storybook/blocks": "8.6.12",
    "@storybook/components": "8.6.12",
    "@storybook/react": "8.6.12",
    "@storybook/react-vite": "8.6.12",
    "@storybook/types": "8.6.12",
    "custom-elements-manifest": "1.0.0",
    "node-fetch": "2.7.0",
    "playwright": "1.56.1",
    "prop-types": "15.8.1",
    "react-docgen": "7.1.1",
    "storybook": "8.6.12",
    "typescript": "5.3.3"
  }
}
