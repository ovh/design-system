{
  "name": "@ovhcloud/ods-themes",
  "version": "19.1.0",
  "description": "All ODS Themes",
  "license": "Apache-2.0",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./default": {
      "sass": "./src/default/default.scss",
      "default": "./dist/default/default.css"
    },
    "./default/css": {
      "sass": "./src/default/index.scss",
      "default": "./dist/default/index.css"
    },
    "./default/fonts": {
      "sass": "./src/default/fonts.scss",
      "default": "./dist/default/fonts.css"
    },
    "./default/tokens": {
      "default": "./dist/default/tokens.json"
    }
  },
  "types": "dist/index.d.ts",
  "scripts": {
    "build:ci": "npm run build:prod",
    "build:prod": "tsc && sass src:dist --no-source-map --pkg-importer=node && postcss dist/**/*.css --replace --use autoprefixer --parser postcss-scss --no-map && npm run generate:tokens",
    "clean": "rimraf dist",
    "generate:tokens": "node scripts/generate-token-lists.js",
    "lint:scss": "stylelint '{assets,src}/**/*.scss'",
    "lint:ts": "eslint 'src/**/*.{js,ts,tsx}'"
  },
  "devDependencies": {
    "autoprefixer": "10.4.17",
    "cssjson": "2.1.3",
    "postcss": "8.4.35",
    "postcss-cli": "11.0.0",
    "postcss-scss": "4.0.9",
    "sass": "1.71.0",
    "typescript": "5.3.3"
  },
  "browserslist": [
    "last 4 version",
    "maintained node versions"
  ]
}
